			game:GetService("TweenService"):Create(uigradient_2, TweenInfo.new(2), {['Rotation'] = 0}):Play()
			task.wait(2)
		end
	end)

	local dragToggle
	local dragInput
	local dragSpeed
	local dragStart
	local dragPos
	local startPos

	local function dragify(Frame)

		dragToggle = nil
		dragSpeed = 0.50
		dragInput = nil
		dragStart = nil
		dragPos = nil

		local function updateInput(input)
			local Delta = input.Position - dragStart
			local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.30), {Position = Position}):Play()
		end

		Frame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UserInputService:GetFocusedTextBox() == nil then
				dragToggle = true
				dragStart = input.Position
				startPos = Frame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)

		Frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)

		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragToggle then
				updateInput(input)
			end
		end)
	end

	-- enable / disable ui --

	local moonEnabled = true

	local function disableTabs(enquiry)
		if enquiry == false then
			tabs.Visible = false
		else
			tabs.Visible = true
		end
	end

	UserInputService.InputBegan:Connect(function(input,Chatting)

		if Chatting then return end -- if player is typing in chat this function will not run

		if input.KeyCode == Enum.KeyCode.RightAlt then -- if player pressed C then this function will run
			if not moonEnabled then
				disableTabs(true)
				moonEnabled = true
			else
				disableTabs(false)
				moonEnabled = false
			end
		end
	end)

	dragify(moon)
	
